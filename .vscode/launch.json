  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "useWSL": true,
      "stopOnEntry": false,
      "sourceMaps": true,
      "args": [],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "port": 5858,
      "console": "internalConsole"
    }
  ]
}
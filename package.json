{
  "name": "js-tdd-kit",
  "version": "1.0.0",
  "description": "ES6 TDD starter kit with Webpack, Babel, ESLint, Testem, Tape, jsdom and Istanbul",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devBuild": "node_modules/.bin/webpack --config webpack/dev",
    "devWatch": "npm run devBuild -- --watch",
    "devStart": "node_modules/.bin/webpack-dev-server --config webpack/dev --open",
    "proBuild": "node_modules/.bin/webpack --config webpack/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfurzewaddock/js-tdd-kit.git"
  },
  "keywords": [
    "ES6",
    "TDD",
    "JavaScript",
    "Testem",
    "Tape",
    "jsdom ",
    "Webpack",
    "Babel",
    "Istanbul",
    "ESLint"
  ],
  "author": "Leo Furze-Waddock <leo@furze-waddock.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfurzewaddock/js-tdd-kit/issues"
  },
  "homepage": "https://github.com/lfurzewaddock/js-tdd-kit#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.0.6",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "^4.1.2"
  }
}

{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-var": "off",
    "quotes": ["error", "double"],
    "prefer-arrow-callback": [ "error", { "allowNamedFunctions": true } ],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["./test/**/*.js", "./webpack/**/*.js"]}],
    "no-param-reassign": ["error", { "props": false }]
  }
}